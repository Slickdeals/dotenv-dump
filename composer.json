{
    "name": "sd/dotenv-dump",
    "description": "Composer plugin to dump Dotenv files",
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "composer-plugin-api": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "SD\\DotenvDump\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SD\\DotenvDump\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "composer/composer": "^1.10.22|^2.0.13",
        "symfony/dotenv": "^3.4|^4.0",
        "phpunit/phpunit": "^7.5"
    },
    "extra": {
        "class": "SD\\DotenvDump\\DotenvDump"
    }
}
